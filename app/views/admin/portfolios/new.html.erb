<div class="form_wrapper">
  <div class="form">
    <% @title = '制作物登録' %>
    <h1><%= @title %></h1>
    <%= form_with model: @portfolio, url: :admin_portfolios do |f| %>
      <%= render 'form', f: f, p: @portfolio %>
      <div class="buttons">
        <%= f.submit '登録' %>
        <%= link_to  "キャンセル", :admin_root %>
      </div>
    <% end %>
  </div>
</div>