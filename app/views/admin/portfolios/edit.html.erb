<div class="form_wrapper">
  <div class="form">
  <% @title = '制作物編集' %>
    <h1><%= @title %></h1>
    <%= form_with model: @portfolio, url: [:admin, @portfolio] do |f| %>
      <%= render 'form', f: f, p: @portfolio %>
      <div class="buttons">
        <%= f.submit '更新' %>
        <%= link_to  "キャンセル", :admin_portfolios %>
      </div>
    <% end %>
  </div>
</div>