<div class="form-group">
    <%= f.label :name, "タイトル" %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :detail, "詳細" %>
    <%= f.text_area :detail, class: 'form-control', rows: 3 %>
  </div>
  <div class="form-group">
    <%= f.label :image, "サムネイル" %>
    <%= f.file_field :image, class: 'form-control', multiple: true %>
  </div>